{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": ["dist/**", ".solid/**"],
            "dependsOn": ["^build"]
        },
        "test": {
            "outputs": ["coverage/**"],
            "dependsOn": []
        },
        "lint": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}
