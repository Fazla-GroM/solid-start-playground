{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "Display": "Solid-Vite",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "jsxImportSource": "solid-js",
        "jsx": "preserve",
        "types": ["vite/client"]
    }
}
