{
    "name": "solid-playground",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "clean": "turbo run clean",
        "dev": "turbo run dev",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "lint": "turbo run lint",
        "test": "turbo run test"
    },
    "devDependencies": {
        "prettier": "^2.7.1",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4",
        "turbo": "latest",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "husky": "^8.0.1",
        "eslint-config-custom": "*"
    },
    "volta": {
        "node": "18.12.0"
    }
}
